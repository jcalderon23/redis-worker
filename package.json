{
  "name": "ael-api-nomina-pila",
  "version": "1.0.0",
  "description": "PILA API project api to AeL (Nomina Pymes)",
  "main": "./dist/src/server.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && tsc",
    "start": "NODE_ENV=production node --require dotenv/config ./dist/src/app.js",
    "start:dev": "nodemon",
    "start:prod": "npm run build && npm run start",
    "lint": "eslint --ignore-path .gitignore . --ext .ts",
    "lint:fix": "npm run lint -- --fix",
    "test": "NODE_ENV=test tsc && jest",
    "seed:config": "ts-node ./node_modules/typeorm-seeding/dist/cli.js config",
    "db:seed": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed"
  },
  "keywords": [],
  "author": "CCXC Camilo Narvaez",
  "license": "ISC",
  "devDependencies": {
    "@faker-js/faker": "7.5.0",
    "@jorgebodega/typeorm-factory": "1.2.0",
    "@types/express": "4.17.13",
    "@types/jest": "27.5.2",
    "@types/node": "17.0.45",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "5.33.1",
    "@typescript-eslint/parser": "5.33.1",
    "better-sqlite3": "7.6.2",
    "eslint": "8.22.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-standard": "17.0.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "26.8.3",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-promise": "6.0.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "faker": "5.5.3",
    "jest": "28.1.3",
    "nodemon": "2.0.20",
    "prettier": "2.7.1",
    "rimraf": "3.0.2",
    "supertest": "6.2.4",
    "ts-jest": "28.0.8",
    "ts-node": "10.9.1",
    "typescript": "4.7.4"
  },
  "dependencies": {
    "axios": "0.27.2",
    "bull": "^4.10.4",
    "cors": "2.8.5",
    "dotenv": "16.0.1",
    "express": "4.18.1",
    "express-validator": "6.14.2",
    "module-alias": "2.2.2",
    "mongodb": "4.11.0",
    "node-cron": "^3.0.2",
    "pg": "8.7.3",
    "reflect-metadata": "0.1.13",
    "swagger-jsdoc": "6.2.5",
    "swagger-ui-express": "4.5.0",
    "throng": "^5.0.0",
    "typeorm": "0.3.7",
    "typeorm-seeding": "1.6.1"
  },
  "_moduleAliases": {
    "@controllers": "dist/src/controllers",
    "@entities": "dist/src/entities",
    "@helpers": "dist/src/helpers",
    "@migrations": "dist/src/databases/migrations",
    "@routes": "dist/src/routes",
    "@services": "dist/src/services",
    "@validators": "dist/src/validators",
    "@traits": "dist/src/traits",
    "@constants": "dist/src/constants"
  },
  "_moduleDirectories": [
    "node_modules_custom"
  ]
}
